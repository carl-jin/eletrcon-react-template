{
  "name": "fb-group-manager-software",
  "description": "FB 找小组本地数据库软件",
  "version": "0.0.1",
  "private": true,
  "author": "CarlJin",
  "homepage": "https://github.com/carl-jin/fb-group-manager-software",
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "serve": "esno scripts/watch.ts",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile:clear": "rm -rf ./dist",
    "compile": "pnpm run build && run-s compile:*",
    "compile:mac": "MODE=production CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder build --config .electron-builder.config.cjs --mac",
    "compile:win": "MODE=production CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder build --config .electron-builder.config.cjs --win",
    "compile:linux": "MODE=production CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder build --config .electron-builder.config.cjs --linux",
    "evd:prepare": "run-s compile:clear build compile:mac",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 node scripts/update-electron-vendors.mjs",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,tsx,ts,json}\"",
    "test": "node ./scripts/test.cjs",
    "checkUpdate": "NPM_CHECK_INSTALLER=pnpm npm-check -u -i"
  },
  "devDependencies": {
    "@ant-design/icons": "^6.1.0",
    "@electron/notarize": "^3.1.1",
    "@electron/osx-sign": "^2.3.0",
    "@inquirer/prompts": "^7.9.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.1.0",
    "antd": "^5.28.0",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "changelog-parser": "^3.0.1",
    "commander": "^14.0.2",
    "cross-env": "^10.1.0",
    "dayjs": "^1.11.19",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "download": "^8.0.0",
    "electron": "^39.1.0",
    "electron-builder": "^26.0.12",
    "esno": "^4.8.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.10",
    "immer": "^10.2.0",
    "jsdom": "^27.1.0",
    "kdbxweb": "^2.1.1",
    "log-symbols": "^7.0.1",
    "marked": "^16.4.1",
    "netlify-cli": "^23.10.0",
    "npm-check": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "p-cancelable": "^4.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^7.9.5",
    "react-use": "^17.6.0",
    "swr": "^2.3.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "unplugin-auto-expose": "^0.3.0",
    "vite": "^7.2.0",
    "vite-plugin-html": "^3.2.2",
    "wrangler": "^4.45.4",
    "zustand": "^5.0.8"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-version-deployer-cli": "^0.4.4",
    "ghost-cursor": "^1.4.1",
    "ghost-cursor-playwright": "^2.1.0",
    "lodash": "^4.17.21",
    "playwright-core": "^1.56.1",
    "sqlite3": "^5.1.7",
    "totp-generator": "^2.0.0",
    "typeorm": "^0.3.27",
    "user-agents": "^1.1.669"
  }
}
